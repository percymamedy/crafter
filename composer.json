{
    "name": "crafter/installer",
    "description": "An all in one PHP framework installer.",
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "6.2.*",
        "illuminate/filesystem": "5.2.*",
        "illuminate/support": "5.2.*",
        "symfony/console": "3.1.*",
        "symfony/process": "3.1.*",
        "symfony/yaml": "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "symfony/var-dumper": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Crafter\\Installer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "bin": [
        "craft"
    ]
}
